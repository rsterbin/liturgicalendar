---
# Config file for the calendar builder

# This file is managed by ansible! Make your changes in the "calendar_builder"
# role and re-run ansible to complete.

# Database config
database:
    drivername: "postgres"
    host: "{{ db_host }}"
    port: {{ db_port }}
    username: "{{ db_owner_name }}"
    password: "{{ db_owner_password }}"
    database: "{{ db_name }}"

# Where do regular sundays of the year fall in precedence?
sunday_precedence: 35

# What precedence do you need to transfer?
transfer_precedence: 42

# AWS config
aws:
    access_key_id: "{{ aws_access_key_id }}"
    secret_access_key: "{{ aws_secret_access_key }}"
    region: "{{ aws_region }}"
    sqs:
        calc_request_url: "{{ aws_sqs_calc_request_url }}"
...
